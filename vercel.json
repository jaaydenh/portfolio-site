{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://blog-site-pi-eight.vercel.app/blog"
    },
    {
      "source": "/blog/:path+",
      "destination": "https://blog-site-pi-eight.vercel.app/blog/:path+"
    },
    {
      "source": "/blog-static/_next/:path+",
      "destination": "https://blog-site-pi-eight.vercel.app/blog-static/_next/:path+"
    }
  ]
}